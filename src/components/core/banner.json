{
  "collectionName": "components_core_banners",
  "info": {
    "displayName": "Banner"
  },
  "options": {},
  "attributes": {
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "title": {
      "type": "text"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "small",
        "big"
      ],
      "required": true,
      "default": "small"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    }
  }
}
